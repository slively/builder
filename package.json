{
  "name": "casker",
  "version": "0.0.7",
  "license": "MIT",
  "repository": "https://github.com/slively/casker",
  "types": "./dist/casker.d.ts",
  "main": "./dist/casker.js",
  "bin": {
    "casker": "./dist/casker-cli.js"
  },
  "scripts": {
    "compile": "rm -rf ./dist && tsc",
